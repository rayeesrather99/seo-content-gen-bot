{
    "version": 2,
    "builds": [
      {
        "src": "backend/content-generator-server.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/build", 
        "use": "@vercel/static"  
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/content-generator-server.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/build/index.html" 
      }
    ]
  }